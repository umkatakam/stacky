<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	
	<ui:with type="com.gwt.stacky.resources.ICardResources" field="resource"></ui:with>
	
	<g:HTMLPanel ui:field="panel" styleName="{resource.getStyle.panel}">
		<g:HTMLPanel ui:field="header" styleName="{resource.getStyle.header}"/>						
		<g:HTMLPanel ui:field="content" styleName="{resource.getStyle.content}"/>			
	</g:HTMLPanel>
</ui:UiBinder> 